{
  "name": "nextjs-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:content && npm run generate-search && npm run seed-chapters && next build",
    "build:content": "bash scripts/build-qmd.sh",
    "generate-search": "node scripts/generate-search-index.mjs",
    "seed-chapters": "test -f .env.local && node --env-file=.env.local scripts/seed-chapters.mjs --run || node scripts/seed-chapters.mjs --run",
    "seed-chapters:sql": "node scripts/seed-chapters.mjs",
    "start": "next start",
    "lint": "eslint",
    "db:reset": "supabase db reset",
    "db:seed-users": "node --env-file=.env.local scripts/seed-test-users.mjs",
    "db:seed-users:force": "node --env-file=.env.local scripts/seed-test-users.mjs --force",
    "db:setup": "npm run db:reset && npm run db:seed-users:force"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@vercel/analytics": "^1.6.1",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "i18n-iso-countries": "^7.14.0",
    "js-yaml": "^4.1.1",
    "katex": "^0.16.27",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-select": "^5.10.2",
    "recharts": "^3.6.0",
    "rehype-katex": "^7.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@clerk/backend": "^2.29.3",
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
